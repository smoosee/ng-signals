{
  "name": "@smoosee/ng-signals",
  "version": "0.0.0",
  "description": "Plug-N-Play manager for Angular Signals.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smoosee/ng-signals.git"
  },
  "keywords": [
    "smoosee",
    "angular",
    "signals",
    "facade",
    "manager"
  ],
  "author": "Mostafa Sherif",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smoosee/ng-signals/issues"
  },
  "homepage": "https://github.com/smoosee/ng-signals#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "************* DEV ****************": "",
    "start": "ng serve demo",
    "watch": "npm run build -- --watch",
    "test": "ng test",
    "************* BUILD ****************": "",
    "prebuild": "indeps pkgVersion",
    "build": "ng build @smoosee/ng-signals",
    "************* RELEASE ****************": "",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs lib/src/index.ts",
    "release": "auto shipit",
    "prepack": "indeps prepack",
    "postpack": "indeps postpack"
  },
  "dependencies": {
    "@angular/animations": "next",
    "@angular/common": "next",
    "@angular/compiler": "next",
    "@angular/core": "next",
    "@angular/forms": "next",
    "@angular/platform-browser": "next",
    "@angular/platform-browser-dynamic": "next",
    "@angular/router": "next",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "next",
    "@angular/cli": "next",
    "@angular/compiler-cli": "next",
    "@smoosee/indeps": "latest",
    "@types/jasmine": "~4.3.0",
    "auto": "^10.45.0",
    "jasmine-core": "~4.6.0",
    "jsdoc-to-markdown": "^8.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.0.0-next.1",
    "typedoc": "^0.24.1",
    "typedoc-plugin-markdown": "^3.15.1",
    "typescript": "~5.0.2"
  },
  "packageManager": "yarn@3.5.0"
}
